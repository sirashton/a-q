{
  "name": "Advices and Queries",
  "private": true,
  "version": "0.0.5",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "overrides": {
    "rimraf": "^5.0.5",
    "glob": "^11.0.0",
    "inflight": "npm:noop2@^2.0.0",
    "@xmldom/xmldom": "^0.8.0",
    "del": "^6.1.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:run && npm run test:e2e",
    "pre-commit": "npm run lint && npm run test:run && npm run build",
    "postinstall": "node scripts/fix-del.cjs",
    "assets:generate": "npx @capacitor/assets generate",
    "assets:copy": "@capacitor/assets copy",
    "deploy:android": "npm run assets:generate && npm run build && npx cap sync && echo 1 | npx cap run android"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/app": "^7.1.0",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/local-notifications": "^7.0.3",
    "@capacitor/preferences": "^7.0.2",
    "@capawesome/capacitor-live-update": "^7.2.2",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/vite": "^4.1.14",
    "capacitor-plugin-safe-area": "^4.0.0",
    "lucide-react": "^0.545.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@playwright/test": "1.56.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "24.6.0",
    "@types/react": "19.1.16",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.4",
    "autoprefixer": "10.4.21",
    "eslint": "9.36.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.22",
    "globals": "16.4.0",
    "jsdom": "27.0.0",
    "opentype.js": "1.3.4",
    "patch-package": "8.0.1",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.14",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0",
    "vite": "7.1.11",
    "vitest": "3.2.4"
  }
}
